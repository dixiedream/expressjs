{
  "description": "Express Js boilerplate",
  "version": "0.0.0",
  "private": true,
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "test": "NODE_ENV=test jest --runInBand",
    "posttest": "npm run lint",
    "coverage": "NODE_ENV=test jest --runInBand --coverage",
    "lint": "ts-standard",
    "fix": "ts-standard --fix",
    "dev": "ts-node-dev --inspect=0.0.0.0 ./bin/www",
    "start": "ts-node ./bin/www.mjs",
    "build": "tsc -p .",
    "old-dev": "NODE_ENV=development nodemon --inspect=0.0.0.0 ./bin/www",
    "old-test": "NODE_ENV=test jest ./tests/integration/* -b --coverage --maxWorkers=1",
    "old-start": "node ./bin/www"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "rest"
  ],
  "author": "Alessandro Lucarini",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^7",
    "i18next": "^23",
    "i18next-fs-backend": "^2",
    "i18next-http-middleware": "^3.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.3",
    "mongoose": "^8",
    "nodemailer": "^6.7.5",
    "typia": "^9.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^6.4.15",
    "jest": "^29.7.0",
    "nodemon": "^3",
    "simple-git-hooks": "^2.9.0",
    "supertest": "^7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.3.0",
    "ts-standard": "^12.0.2"
  }
}
