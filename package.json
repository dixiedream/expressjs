    "i18next": "^23",
    "i18next-fs-backend": "^2",
    "i18next-http-middleware": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.3",
    "mongoose": "^8",
    "nodemailer": "^6.7.5",
    "typia": "^9.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
